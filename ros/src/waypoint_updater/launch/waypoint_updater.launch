<?xml version="1.0"?>
<launch>
    <node pkg="waypoint_updater" type="waypoint_updater.py" name="waypoint_updater" >
        <param name="lookahead_wps" value="15" />
        <param name="waypoint_update_frequency" value="50" />
        <param name="traffic_light_full_stop_distance" value="1." />
        <param name="traffic_light_over_distance" value="2." />
        <param name="acceleration_start_velocity" value="4" />
        <param name="acceleration_distance" value="20" />
    </node>
</launch>
